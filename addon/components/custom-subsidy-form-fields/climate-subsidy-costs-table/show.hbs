<ul class="au-o-grid">
  <li class="au-o-grid__item au-u-1-2 au-u-1-6@medium">
    <label class="au-c-label ">Aantal inwoners</label>
    <p>{{this.populationCount}}</p>
  </li>
  <li class="au-o-grid__item au-u-1-2 au-u-2-6@medium">
        <label class="au-c-label ">Totaal trekkingsrecht</label>
        <p>{{convert-to-currency this.drawingRight}}</p>
      </li>
      <li class="au-o-grid__item au-u-1-2 au-u-2-6@medium">
        <label class="au-c-label ">Trekkingsrecht te verdelen</label>
        <p style={{if this.errors "color:#D92626"}}>{{convert-to-currency this.restitutionToDestribute}}</p>
      </li>
</ul>
<br>
<br>
<AuLabel @error={{this.hasErrors}} for={{this.inputFor}}>
  {{@field.label}}
</AuLabel>
{{#each this.errors as |error|}}
    <AuHelpText @error={{true}}>{{error.message}}</AuHelpText>
{{/each}}
