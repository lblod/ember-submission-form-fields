    sh:name "Text area" ;
    sh:order 230 ;
    sh:path ext:textAreaValue ;
    form:displayType displayTypes:textArea ;
    form:validatedBy
    [ a form:MaxLength ;
      form:grouping form:MatchEvery ;
      form:max "100" ;
      sh:resultMessage "Max. karakters overschreden." ;
      sh:path ext:textAreaValue ];
    sh:group ext:mainPg .

ext:mainFg form:hasField ext:textAreaField .

