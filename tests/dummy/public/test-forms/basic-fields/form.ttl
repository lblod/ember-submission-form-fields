##########################################################
# Search
##########################################################
ext:searchField a form:Field ;
    mu:uuid "a43c7bfe-0a12-4645-aa6d-44b24eddbd3f";
    sh:name "Search";
    sh:order 330;
    sh:path ext:searchValue;
    form:displayType displayTypes:search ;
